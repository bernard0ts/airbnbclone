<div style="text-align: center;">
  <p style="color: green;"><%= notice %></p>

  <div id="apartments">
    <% @apartments.each do |apartment| %>
      <%= render apartment %>
      <p>
        <%= link_to "Show this apartment", apartment%>
      </p>
    <% end %>
  </div>

  <%= link_to "New apartment", new_apartment_path %>
</div>
